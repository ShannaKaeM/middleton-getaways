        },
        {
          "color": "#5a6b2e",
          "name": "Primary Dark",
          "slug": "primary-dark"
        },
        {
          "color": "#7d96a6",
          "name": "Secondary Light",
          "slug": "secondary-light"
        },
        {
          "color": "#616161",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#495057",
          "name": "Secondary Dark",
          "slug": "secondary-dark"
        },
        {
          "color": "#e8e6e0",
          "name": "Neutral Light",
          "slug": "neutral-light"
        },
        {
          "color": "#b5b09f",
          "name": "Neutral",
          "slug": "neutral"
        },
        {
          "color": "#8a8577",
          "name": "Neutral Dark",
          "slug": "neutral-dark"
        },
        {
          "color": "#f8f9fa",
          "name": "Base Lightest",
          "slug": "base-lightest"
        },
        {
          "color": "#d1d5db",
          "name": "Base Light",
          "slug": "base-light"
        },
        {
          "color": "#616161",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#495057",
          "name": "Base Dark",
          "slug": "base-dark"
        },
        {
          "color": "#343a40",
          "name": "Base Darkest",
          "slug": "base-darkest"
        },
        {
          "color": "#ffffff",
          "name": "Extreme Light",
          "slug": "extreme-light"
        },
        {
          "color": "#1a1a1a",
          "name": "Extreme Dark",
          "slug": "extreme-dark"
        }
      ]
    },
    "typography": {
      "fontSizes": [
        {
          "size": "0.75rem",
          "slug": "xs",
          "name": "Extra Small"
        },
        {
          "size": "0.875rem",
          "slug": "sm",
          "name": "Small"
        },
        {
          "size": "1rem",
          "slug": "base",
          "name": "Base"
        },
        {
          "size": "1.125rem",
          "slug": "lg",
          "name": "Large"
        },
        {
          "size": "1.25rem",
          "slug": "xl",
          "name": "Extra Large"
        },
        {
          "size": "1.5rem",
          "slug": "2xl",
          "name": "2X Large"
        },
        {
          "size": "2rem",
          "slug": "3xl",
          "name": "3X Large"
        },
        {
          "size": "3rem",
          "slug": "4xl",
          "name": "4X Large"
        },
        {
          "size": "4rem",
          "slug": "5xl",
          "name": "5X Large"
        }
      ],
      "fontWeight": true,
      "lineHeight": true
    },
    "border": {
      "radius": true
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--extreme-light)",
      "text": "var(--wp--preset--color--base-darkest)"
    },
    "typography": {
      "fontFamily": "inherit",
      "fontSize": "var(--wp--preset--font-size--base)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--md)"
    },
    "elements": {
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--base-darkest)"
        },
        "typography": {
          "fontWeight": "500",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--4xl)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--3xl)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--2xl)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xl)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--lg)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--base)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-hover)"
          }
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--extreme-light)"
        },
        "border": {
          "radius": "8px"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--sm)",
            "right": "var(--wp--preset--spacing--lg)",
            "bottom": "var(--wp--preset--spacing--sm)",
            "left": "var(--wp--preset--spacing--lg)"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary-hover)"
          }
        }
      }
    }
  },
  "customTemplates": [],
  "templateParts": []
}
