{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "layout": {
      "contentSize": "1280px",
      "wideSize": "1400px"
    },
    "spacing": {
      "spacingSizes": [
        {
          "size": "0.5rem",
          "slug": "xs",
          "name": "Extra Small"
        },
        {
          "size": "0.75rem",
          "slug": "sm", 
          "name": "Small"
        },
        {
          "size": "1rem",
          "slug": "md",
          "name": "Medium"
        },
        {
          "size": "1.5rem",
          "slug": "lg",
          "name": "Large"
        },
        {
          "size": "2rem",
          "slug": "xl",
          "name": "Extra Large"
        },
        {
          "size": "3rem",
          "slug": "2xl",
          "name": "2X Large"
        },
        {
          "size": "4rem",
          "slug": "3xl",
          "name": "3X Large"
        }
      ]
    },
    "color": {
      "palette": [
        {
          "color": "#a0b05e",
          "name": "Primary Light",
